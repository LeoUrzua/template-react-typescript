{
  "author": "Leo Urzua <d.leonardo.urzua@gmail.com>",
  "bugs": {
    "url": "https://github.com/LeoUrzua/template-react-typescript/issues"
  },
  "description": "template for react projects",
  "devDependencies": {
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "homepage": "https://github.com/LeoUrzua/template-react-typescript#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged"
    }
  },
  "keywords": [
    "template",
    "react",
    "typescript"
  ],
  "license": "ISC",
  "lint-staged": {
    "*.{js,css,json,md}": [
      "pretty-quick --staged",
      "git add"
    ],
    "*.{ts, tsx}": [
      "npm run tslint",
      "pretty-quick --staged",
      "git add"
    ]
  },
  "main": "index.js",
  "name": "template-react-typescript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeoUrzua/template-react-typescript.git"
  },
  "scripts": {
    "tslint": "echo \"Success: mock tslint response\" && exit 0",
    "test": "echo \"Success: mock  test response\" && exit 0"
  },
  "version": "0.0.0"
}
